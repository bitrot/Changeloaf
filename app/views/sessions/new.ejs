<div class="page-header">
    <h1>New session</h1>
</div>

<% form_for(session, {action: path_to.sessions, method: 'POST', id: "session_form", class: 'form-horizontal'}, function (form) { %>
    <%- partial('sessions/form.ejs', {locals: {form: form, session: session}}) %>
    <div class="form-actions">
        <%- form.submit('<i class="icon-ok icon-white"></i> Create session', {class: 'btn btn-primary'}) %> or
        <%- link_to('Cancel', path_to.sessions, {class: 'btn'}) %>
    </div>
<% });%>
